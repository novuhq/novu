      "**/react-router",
      "**/react-router/**",
      "**/react-router-dom",
      "**/react-router-dom/**",
      "**/@cypress",
      "**/@storybook/**",
      "**/@storybook",
      "**/@craco",
      "**/@craco/**",
      "**/@babel",
      "**/@babel/**"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
