      "**/@cypress",
      "**/@storybook/**",
      "**/@storybook",
      "**/@babel",
      "**/@babel/**"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.11"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
