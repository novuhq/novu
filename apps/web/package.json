  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "workspaces": {
    "nohoist": [
      "**/@ant-design/icons",
      "**/@ant-design/icons/**",
      "**/antd",
      "**/antd/**",
      "**/react-scripts",
      "**/react-scripts/**",
