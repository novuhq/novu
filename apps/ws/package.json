    "@nestjs/swagger": "7.4.0",
    "@nestjs/terminus": "10.2.3",
    "@nestjs/websockets": "10.4.1",
    "@novu/application-generic": "workspace:*",
    "@novu/dal": "workspace:*",
    "@novu/shared": "workspace:*",
    "@novu/testing": "workspace:*",
    "@sentry/node": "^7.30.0",
