    "@socket.io/redis-adapter": "^7.2.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "helmet": "^6.0.1",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.0",
    "lodash": "^4.17.15",
    "nest-raven": "10.1.0",
    "newrelic": "^9.15.0",
