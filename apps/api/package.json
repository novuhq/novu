    "start:test": "cross-env NODE_ENV=test PORT=1336 TZ=UTC nodemon",
    "start:debug": "TZ=UTC nodemon --config nodemon-debug.json",
    "start:prod": "TZ=UTC node dist/main.js",
    "lint": "eslint src",
    "lint:fix": "pnpm lint -- --fix",
    "test": "cross-env TZ=UTC NODE_ENV=test mocha --timeout 10000 --require ts-node/register --exit --file e2e/setup.ts src/**/**/*.spec.ts",
