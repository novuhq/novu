{
  "name": "@novu/worker",
  "version": "2.0.0",
  "description": "description",
  "author": "",
  "private": "true",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "pnpm --silent --workspace-root pnpm-context -- apps/worker/Dockerfile | BULL_MQ_PRO_NPM_TOKEN=${BULL_MQ_PRO_NPM_TOKEN} docker buildx build --secret id=BULL_MQ_PRO_NPM_TOKEN --build-arg PACKAGE_PATH=apps/worker - -t novu-worker --load $DOCKER_BUILD_ARGUMENTS",
    "docker:build:depot": "pnpm --silent --workspace-root pnpm-context -- apps/worker/Dockerfile | depot build --build-arg PACKAGE_PATH=apps/worker - -t novu-worker --load",
    "start": "pnpm start:dev",
    "start:dev": "nest start --watch",
    "start:test": "cross-env NODE_ENV=test pnpm start:prod",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint src",
    "lint:fix": "pnpm lint -- --fix",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{\"strictNullChecks\": false}' NODE_ENV=test E2E_RUNNER=true mocha --timeout 10000 --require ts-node/register --exit --file e2e/setup.ts src/**/**/*.spec.ts",
    "test:e2e": "cross-env TS_NODE_COMPILER_OPTIONS='{\"strictNullChecks\": false}' NODE_ENV=test E2E_RUNNER=true mocha --timeout 10000 --require ts-node/register --exit --file e2e/setup.ts src/**/*.e2e.ts"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.3",
    "@nestjs/common": "10.4.1",
    "@nestjs/core": "10.4.1",
    "@nestjs/platform-express": "10.4.1",
    "@nestjs/swagger": "7.4.0",
    "@nestjs/terminus": "10.2.3",
    "@novu/application-generic": "workspace:*",
    "@novu/dal": "workspace:*",
    "@novu/shared": "workspace:*",
    "@novu/stateless": "workspace:*",
    "@novu/framework": "workspace:*",
    "@novu/testing": "workspace:*",
    "@sentry/node": "^7.40.0",
    "@sentry/tracing": "^7.40.0",
    "@types/newrelic": "^9.13.0",
    "axios": "^1.6.8",
    "body-parser": "^1.20.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "cron-parser": "^4.9.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.2",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "helmet": "^6.0.1",
    "i18next": "^23.7.6",
    "inline-css": "^4.0.2",
    "ioredis": "^5.2.4",
    "lodash": "^4.17.15",
    "nest-raven": "10.1.0",
    "newrelic": "^9.15.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "^3.0.2",
    "rxjs": "7.8.1",
    "shortid": "^2.2.16",
    "simple-statistics": "^7.8.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@nestjs/cli": "10.4.5",
    "@nestjs/schematics": "10.1.4",
    "@nestjs/testing": "10.4.1",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.11",
    "@types/express": "4.17.17",
    "@types/inline-css": "^3.0.3",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.15.0",
    "@types/sinon": "^9.0.0",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "mocha": "^10.2.0",
    "sinon": "^9.2.4",
    "superagent-defaults": "^0.1.14",
    "supertest": "^7.0.0",
    "ts-loader": "~9.4.0",
    "ts-node": "~10.9.1",
    "tsconfig-paths": "~4.1.0",
    "typescript": "5.6.2"
  },
  "optionalDependencies": {
    "@novu/ee-auth": "workspace:*",
    "@novu/ee-billing": "workspace:*",
    "@novu/ee-shared-services": "workspace:*",
    "@novu/ee-translation": "workspace:*"
  }
}
